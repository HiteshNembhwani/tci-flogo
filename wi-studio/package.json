  "description": "TIBCO Cloud Integration, web integrator studio project",
  "scripts": {
    "start": "WISTUDIO_SERVER_MODE=${MODE:-\"prod\"} node dist/server/main.js",
    "debug": "WISTUDIO_SERVER_MODE=${MODE:-\"test\"} node debug dist/server/main.js",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "tsc": "tsc",
    "webpack": "webpack --config ./webpack.config.js --progress",
    "parser": "pushd parser; npm install; popd; ",
    "xpreinstall": "npm run parser",
    "tsc:w": "tsc -w",
    "i18n": "ng-xi18n",
    "rollup": "rollup -c rollup.config.js",
    "dev": "gulp && concurrently \"tsc -w -p ./client\" \"lite-server -c ./bs-config\"",
    "test-server": "WISTUDIO_SERVER_MODE=${MODE:-\"test\"} mocha --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/test-results.xml --timeout 15000 dist/server/test/**/*.spec.js"
  },
